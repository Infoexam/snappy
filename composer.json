{
  "name": "infoexam/snappy",
  "description": "Infoexam snappy.",
  "type": "library",
  "homepage": "https://github.com/Infoexam/snappy",
  "license": "MIT",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "freedom@csie.io"
    }
  ],
  "require": {
    "php": "^7.0",
    "barryvdh/laravel-snappy": "^0.3",
    "h4cc/wkhtmltoimage-amd64": "^0.12",
    "h4cc/wkhtmltoimage-i386": "^0.12",
    "h4cc/wkhtmltopdf-amd64": "^0.12",
    "h4cc/wkhtmltopdf-i386": "^0.12",
    "illuminate/support": "5.3.*"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "^0.3",
    "orchestra/testbench": "3.3.*",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Infoexam\\Snappy\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "notify-on-install": false
  }
}
