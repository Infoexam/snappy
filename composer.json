{
  "name": "infoexam/snappy",
  "description": "CCU Infoexam snappy package.",
  "version": "1.0.1",
  "type": "library",
  "homepage": "https://github.com/infoexam/snappy",
  "license": "MIT",
  "authors": [
    {
      "name": "bepsvpt",
      "email": "og7lsrszah6y3lz@infinitefa.email"
    }
  ],
  "require": {
    "php": "^7.4",
    "barryvdh/laravel-snappy": "^0.4",
    "h4cc/wkhtmltoimage-amd64": "^0.12",
    "h4cc/wkhtmltoimage-i386": "^0.12",
    "h4cc/wkhtmltopdf-amd64": "^0.12",
    "h4cc/wkhtmltopdf-i386": "^0.12",
    "illuminate/support": "^6.8"
  },
  "require-dev": {
    "orchestra/testbench": "^4.4",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Infoexam\\Snappy\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "notify-on-install": false,
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
